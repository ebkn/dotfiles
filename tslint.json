{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "no-any": false,
    "semicolon": [true, "always"],
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [true, "never-prefix"],
    "variable-name": [true, "allow-pascal-case"],
    "import-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false
  }
}
