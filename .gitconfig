[user]
	name = ebkn
	email = ktennis.mqekr12@gmail.com
  signingkey = CC3F5520A56190D3165E1C3029749494F46DD884

[github]
	user = ebkn

[core]
	excludesfile = /Users/kenichi/.gitignore_global
	whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol

[gpg]
	program = gpg

[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true

[commit]
	template = /Users/kenichi/.stCommitMsg
	gpgsign = true

[fetch]
	prune = true

[apply]
	whitespace = nowarn

[log]
	date = short

[color]
	ui = true
	branch = auto
	diff = auto
	status = auto
	showbranch = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan
