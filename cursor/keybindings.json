// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpen",
    "when": "!editorFocus && !terminalFocus"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpen",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+f",
    "command": "-editor.action.pageDownHover"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-vim.ctrl-f"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-vim.send"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.findInFiles",
    "when": "!editorFocus && !terminalFocus"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.findInFiles",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+g",
    "command": "-workbench.action.gotoLine"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send-cmdline"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send"
  },
  {
    "key": "s v",
    "command": "workbench.action.splitEditor",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s s",
    "command": "workbench.action.splitEditorDown",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s q",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s k",
    "command": "workbench.action.focusAboveGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s j",
    "command": "workbench.action.focusBelowGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+f",
    "command": "-cursorRight"
  },
  {
    "key": "ctrl+f",
    "command": "cursorRight",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+b",
    "command": "cursorLeft",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+n",
    "command": "cursorBottom",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+p",
    "command": "cursorTop",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+a",
    "command": "cursorLineStart",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+c",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+s",
    "command": "-vscode-vim.send",
    "when": "editorTextFocus && vim.ctrlKeysNormal.s && vim.init && vim.mode != 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "ctrl+s",
    "command": "-vscode-vim.send",
    "when": "editorTextFocus && vim.ctrlKeysInsert.s && vim.init && vim.mode == 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "cmd+t",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "cmd+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.terminal.split"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
    "when": "inQuickOpen && inViewsPicker"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "r n",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename"
  },
  {
    "key": "ctrl+d",
    "command": "-list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "-editor.action.pageDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl+d",
    "command": "-vscode-vim.ctrl-d",
    "when": "editorTextFocus && vim.ctrlKeysNormal.d && vim.init && vim.mode != 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight"
  },
  {
    "key": "ctrl+d",
    "command": "deleteRight",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "alt+q",
    "command": "-rewrap.rewrapComment",
    "when": "editorTextFocus"
  }
]
