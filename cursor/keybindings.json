// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpen",
    "when": "!editorFocus && !terminalFocus"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.action.quickOpen",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+f",
    "command": "-editor.action.pageDownHover"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-vim.ctrl-f"
  },
  {
    "key": "ctrl+f",
    "command": "-vscode-vim.send"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.findInFiles",
    "when": "!editorFocus && !terminalFocus"
  },
  {
    "key": "ctrl+g",
    "command": "workbench.action.findInFiles",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+g",
    "command": "-workbench.action.gotoLine"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send-cmdline"
  },
  {
    "key": "ctrl+g",
    "command": "-vscode-vim.send"
  },
  {
    "key": "s v",
    "command": "workbench.action.splitEditor",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s s",
    "command": "workbench.action.splitEditorDown",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s q",
    "command": "workbench.action.closeActiveEditor",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s k",
    "command": "workbench.action.focusAboveGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s j",
    "command": "workbench.action.focusBelowGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s h",
    "command": "workbench.action.focusLeftGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "s l",
    "command": "workbench.action.focusRightGroup",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+f",
    "command": "-cursorRight"
  },
  {
    "key": "ctrl+f",
    "command": "cursorRight",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+b",
    "command": "cursorLeft",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+n",
    "command": "cursorDown",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+p",
    "command": "cursorUp",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+a",
    "command": "cursorLineStart",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "ctrl+c",
    "command": "editor.action.commentLine",
    "when": "editorTextFocus && !editorReadonly"
  },
  {
    "key": "ctrl+s",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+s",
    "command": "-vscode-vim.send",
    "when": "editorTextFocus && vim.ctrlKeysNormal.s && vim.init && vim.mode != 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "ctrl+s",
    "command": "-vscode-vim.send",
    "when": "editorTextFocus && vim.ctrlKeysInsert.s && vim.init && vim.mode == 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "cmd+b",
    "command": "-workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "cmd+0",
    "command": "-workbench.action.focusSideBar"
  },
  {
    "key": "ctrl+t",
    "command": "workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal",
    "when": "terminal.active"
  },
  {
    "key": "ctrl+q v",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus && terminalProcessSupported || terminalFocus && terminalWebExtensionContributedProfile"
  },
  {
    "key": "cmd+t",
    "command": "-workbench.action.showAllSymbols"
  },
  {
    "key": "cmd+\\",
    "command": "-workbench.action.terminal.split"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
    "when": "inQuickOpen && inViewsPicker"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenNavigateNextInEditorPicker",
    "when": "inEditorsPicker && inQuickOpen"
  },
  {
    "key": "ctrl+tab",
    "command": "-workbench.action.quickOpenPreviousRecentlyUsedEditorInGroup",
    "when": "!activeEditorGroupEmpty"
  },
  {
    "key": "r n",
    "command": "editor.action.rename",
    "when": "editorHasRenameProvider && editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "f2",
    "command": "-editor.action.rename"
  },
  {
    "key": "ctrl+d",
    "command": "-list.focusPageDown",
    "when": "listFocus && !inputFocus"
  },
  {
    "key": "ctrl+d",
    "command": "-editor.action.pageDownHover",
    "when": "editorHoverFocused"
  },
  {
    "key": "ctrl+d",
    "command": "-vscode-vim.ctrl-d",
    "when": "editorTextFocus && vim.ctrlKeysNormal.d && vim.init && vim.mode != 'Insert' && editorLangId not in 'vim.editorLangIdExclusions'"
  },
  {
    "key": "ctrl+d",
    "command": "-deleteRight"
  },
  {
    "key": "ctrl+d",
    "command": "deleteRight",
    "when": "editorTextFocus && vim.mode == 'Insert'"
  },
  {
    "key": "alt+q",
    "command": "-rewrap.rewrapComment",
    "when": "editorTextFocus"
  },
  {
    "key": "g r",
    "command": "editor.action.goToReferences",
    "when": "editorHasReferenceProvider && editorTextFocus && !inReferenceSearchEditor && !isInEmbeddedEditor && editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "shift+f12",
    "command": "-editor.action.goToReferences"
  },
  {
    "key": "g d",
    "command": "editor.action.revealDefinition",
    "when": "editorHasDefinitionProvider && editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "f12",
    "command": "-editor.action.revealDefinition"
  },
  {
    "key": "cmd+f12",
    "command": "-editor.action.revealDefinition"
  },
  {
    "key": "g i",
    "command": "editor.action.goToImplementation",
    "when": "editorHasImplementationProvider && editorTextFocus && editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "cmd+f12",
    "command": "-editor.action.goToImplementation"
  },
  {
    "key": "g g",
    "command": "cursorTop",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "shift+g",
    "command": "cursorBottom",
    "when": "editorTextFocus && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+q h",
    "command": "workbench.action.terminal.focusPrevious",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "shift+cmd+[",
    "command": "-workbench.action.terminal.focusPrevious"
  },
  {
    "key": "ctrl+q l",
    "command": "workbench.action.terminal.focusNext",
    "when": "terminalFocus && terminalHasBeenCreated && !terminalEditorFocus || terminalFocus && terminalProcessSupported && !terminalEditorFocus"
  },
  {
    "key": "shift+cmd+]",
    "command": "-workbench.action.terminal.focusNext"
  },
  {
    "key": "ctrl+q l",
    "command": "workbench.action.terminal.focusNextPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+down",
    "command": "-workbench.action.terminal.focusNextPane"
  },
  {
    "key": "ctrl+q h",
    "command": "workbench.action.terminal.focusPreviousPane",
    "when": "terminalFocus && terminalHasBeenCreated || terminalFocus && terminalProcessSupported"
  },
  {
    "key": "alt+cmd+up",
    "command": "-workbench.action.terminal.focusPreviousPane"
  },
  {
    "key": "ctrl+shift+5",
    "command": "-workbench.action.terminal.split"
  },
  {
    "key": "delete",
    "command": "-workbench.action.terminal.killActiveTab"
  },
  {
    "key": "cmd+w",
    "command": "-workbench.action.terminal.killEditor"
  },
  {
    "key": "ctrl+q q",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "cmd+w",
    "command": "workbench.action.closeActiveEditor",
    "when": "!terminalFocus"
  },
  {
    "key": "shift+n",
    "command": "explorer.newFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus && !treeFindOpen"
  },
  {
    "key": "shift+d",
    "command": "deleteFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceReadonly && !inputFocus && !treeFindOpen"
  },
  {
    "key": "alt+cmd+backspace",
    "command": "-deleteFile"
  },
  {
    "key": "cmd+backspace",
    "command": "-deleteFile"
  },
  {
    "key": "m",
    "command": "renameFile",
    "when": "filesExplorerFocus && foldersViewVisible && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "enter",
    "command": "-renameFile"
  },
  {
    "key": "ctrl+o",
    "command": "-lineBreakInsert",
    "when": "textInputFocus && !editorReadonly"
  },
  {
    "key": "cmd+i",
    "command": "composerMode.agent"
  }
]
